case 'TOGGLE_EDIT_FORM':\r\n          return {\r\n              ...state,\r\n              editFormVisible: visible,\r\n              editId: editId\r\n          }\r\n      case 'EDIT_BLOG':\r\n          return {\r\n              ...state,\r\n              blogs: state.blogs.map(myBlog => \r\n                (myBlog.id === id)\r\n              ? { url, id, title, text } \r\n              : myBlog)\r\n          }\r\n      case 'DELETE_BLOG':\r\n          return {\r\n            ...state,\r\n            blogs: state.blogs.filter(b => b.id !== id)\r\n          }\r\n      default:\r\n        return state\r\n    }\r\n  }\r\n  \r\n  export default blog"},"lineCount":null}},"error":null,"hash":"d1a8639e47f53f888c2aa955b2097499","cacheData":{"env":{}}}